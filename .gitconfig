[core]
    excludefiles = ~/.gitignore
    editor = 'emacs -nw'
    pager = less

[color]
    ui = auto
    grep = auto

[alias]
    br = branch
    st = status
    co = checkout
    ci = commit
    k = !gitk
    g = gui
    last = log -1 HEAD
    onel = log --oneline --decorate
    onelx = log --oneline --decorate -30 HEAD
    graph = log --oneline --graph --pretty='format:|%C(yellow) %h | %C(red) %ad | %C(green) %an | %C(reset) %s %d'

[user]
    name = YYY
    email = XXX
[credential]
    helper = cache --timeout=3600

